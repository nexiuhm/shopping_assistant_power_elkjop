{
    "name": "Playstation 5 kjøpsassistent",

    "description": "Kjøpe assistent for Playstation 5. Et verktøy som kan brukes i kampen mot storkjøpere, som har preget lanseringen i Norge.",

    "version": "0.0.3",

    "manifest_version": 2,

    "icons": { "16": "./icons/16x16.png",
           "48": "./icons/48x48.png",
          "128": "./icons/128x128.png" },

    

    "background": {
        "scripts": ["./background.js"]
    },

    "options_page": "./options.html",

    "browser_action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "webNavigation",
        "https://www.power.no/",
        "https://www.netonnet.no/",
        "https://www.elkjop.no/",
        "https://api.vipps.no/"

    ] 
}
